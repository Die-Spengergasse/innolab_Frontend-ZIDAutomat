{
  "openapi": "3.0.1",
  "info": {
    "title": "ZID.Automat.Api",
    "version": "1.0"
  },
  "paths": {
    "/Account/Login": {
      "post": {
        "tags": [
          "Account"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserLogin"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/UserLogin"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/UserLogin"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ADUser"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ADUser"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ADUser"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ADUser": {
        "type": "object",
        "properties": {
          "firstname": {
            "type": "string",
            "nullable": true
          },
          "lastname": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "cn": {
            "type": "string",
            "nullable": true
          },
          "dn": {
            "type": "string",
            "nullable": true
          },
          "groupMemberhips": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "pupilId": {
            "type": "string",
            "nullable": true
          },
          "teacherId": {
            "type": "string",
            "nullable": true
          },
          "classes": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true,
            "readOnly": true
          },
          "role": {
            "$ref": "#/components/schemas/ADUserRole"
          }
        },
        "additionalProperties": false
      },
      "ADUserRole": {
        "enum": [
          "Other",
          "Pupil",
          "Teacher",
          "Administration",
          "Management"
        ],
        "type": "string"
      },
      "UserLogin": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}