{
  "name": "zidautomat-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-api && ng build AutomatShared && npm-run-all  --parallel build-shared-watch start-user start-admin ",
    "start-user": "ng serve UserFrontend",
    "start-admin": "ng serve AdminFrontend",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate-api-client-File": "ng-openapi-gen --input openapi/swagger.json -o projects/automat-api/src/API | ng build AutomatApi",
    "generate-api-client-Swagger": "curl.exe --output openApi/swagger.json --url http://localhost:5141/swagger/v1/swagger.json | npm run generate-api-client-File",
    "build-all": "npm-run-all --sequential build-shared build-api build-userfrontend build-adminfrontend",
    "build-shared": "ng build AutomatShared",
    "build-api": "ng build AutomatShared",
    "build-shared-watch": "ng build AutomatShared --watch",
    "build-userfrontend": "ng build UserFrontend",
    "build-adminfrontend": "ng build AdminFrontend"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/file-input": "^7.0.1",
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@auth0/angular-jwt": "^5.1.0",
    "@ctrl/ngx-chartjs": "^8.0.0",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@swimlane/ngx-datatable": "^20.1.0",
    "angularx-qrcode": "^13.0.15",
    "chart.js": "^3.6.2",
    "color-convert": "^2.0.1",
    "flat-colors": "^3.3.12",
    "font-awesome": "^4.7.0",
    "ngx-chartjs": "^0.0.1",
    "pexels": "^1.4.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.9",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.12",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.6.2"
  }
}
